{
  "name": "draw-exporter",
  "productName": "draw-exporter",
  "version": "1.1.11",
  "description": "Draw.io exporter",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "test": "tsc && jest --ci --reporters=default --reporters=jest-junit",
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "Lionel Molinier",
    "email": "lionel@molinier.eu"
  },
  "license": "MIT",
  "config": {
    "forge": "./dist/forge.config.js"
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testRunner": "jest-circus/runner",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "@types/commander": "^2.12.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/pako": "^1.0.2",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/xml-parser": "^1.2.30",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "canvas": "^2.8.0",
    "electron": "16.0.3",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "pdfjs-dist": "^2.10.377",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "prettier": "^2.4.1",
    "svg-png-converter": "^0.0.9",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "pako": "^2.0.4",
    "typescript-logging": "^1.0.0",
    "xml2js": "^0.4.23"
  }
}
